	keywords = {collaboration, communication, computer-supported cooperative work, mypapers:conference},
	pages = {225–234},
	file = {ACM Snapshot:/Users/pwagstro/Library/Application Support/Zotero/Profiles/lxtmlfpa.default/zotero/storage/JTDXSBCP/citation.html:text/html;Dabbish_2010_CoordinationInInnovativeDesignAndEngineeringObservationsFromALunarRoboticsProject.pdf:/Users/pwagstro/Library/Application Support/Zotero/Profiles/lxtmlfpa.default/zotero/storage/S2T93FQ3/Dabbish_2010_CoordinationInInnovativeDesignAndEngineeringObservationsFromALunarRoboticsProject.pdf:application/pdf}
