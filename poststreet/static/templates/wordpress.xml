<rss version="2.0"
        xmlns:content="http://purl.org/rss/1.0/modules/content/"
        xmlns:wfw="http://wellformedweb.org/CommentAPI/"
        xmlns:dc="http://purl.org/dc/elements/1.1/"
        xmlns:wp="http://wordpress.org/export/1.0/"
>

<channel>
        <title>$weblog.title</title>
        <link>http://patrick.wagstrom.net/wp</link>
        <description>Thoughts of a Cautious Technocrat</description>
        <pubDate>Wed, 10 Dec 2008 05:44:04 +0000</pubDate>
        <generator>PostStreet</generator>
        <language>en</language>
        <wp:wxr_version>1.0</wp:wxr_version>
        <wp:base_site_url>http://patrick.wagstrom.net/wp</wp:base_site_url>
        <wp:base_blog_url>http://patrick.wagstrom.net/wp</wp:base_blog_url>

        #for $category in $categories
        <wp:category><wp:category_nicename>$category.nice_name</wp:category_nicename><wp:category_parent>$category.nice_parent</wp:category_parent><wp:cat_name><![CDATA[$category.name]]></wp:cat_name></wp:category>
        #end for

        #for $tag in $tags
        <wp:tag><wp:tag_slug>$tag.nice_name</wp:tag_slug><wp:tag_name><![CDATA[$tag.name]]></wp:tag_name></wp:tag>
        #end for
        
#for $entry in $entries
                <item>
<title>$entry.title</title>
<link>http://$SERVER_NAME$CGI_URL/$entry.permalink()</link>
<pubDate>$entry.isoPubDate()</pubDate>
<dc:creator><![CDATA[patrick]]></dc:creator>

                <category><![CDATA[$entry.category.name]]></category>

                <category domain="category" nicename="$entry.category.nice_name"><![CDATA[$entry.category.name]]></category>

                #for $tag in $entry.tags
                <category domain="tag"><![CDATA[$tag.name]]></category>
                <category domain="tag" nicename="$tag.nice_name"><![CDATA[$tag.name]]></category>
                #end for

<guid isPermaLink="false">http://patrick.wagstrom.net/wp/?p=$entry.id</guid>
<description></description>
<content:encoded><![CDATA[$entry.translatedBody]]></content:encoded>
<excerpt:encoded><![CDATA[]]></excerpt:encoded>
<wp:post_id>$entry.id</wp:post_id>
<wp:post_date>$entry.post_date</wp:post_date>
<wp:post_date_gmt>$entry.gmt_post_date</wp:post_date_gmt>
<wp:comment_status>open</wp:comment_status>
<wp:ping_status>open</wp:ping_status>
<wp:post_name>$entry.name</wp:post_name>
<wp:status>publish</wp:status>
<wp:post_parent>0</wp:post_parent>
<wp:menu_order>0</wp:menu_order>
<wp:post_type>post</wp:post_type>
<wp:post_password></wp:post_password>


        #for $comment in $entry.comments
        <wp:comment>
        <wp:comment_id>$comment.id</wp:comment_id>
        <wp:comment_author><![CDATA[$comment.name]]></wp:comment_author>
        <wp:comment_author_email>$comment.email</wp:comment_author_email>
        <wp:comment_author_url>$comment.url</wp:comment_author_url>
        <wp:comment_author_IP>$comment.remoteAddr</wp:comment_author_IP>
        <wp:comment_date>$comment.post_date</wp:comment_date>
        <wp:comment_date_gmt>$comment.gmt_post_date</wp:comment_date_gmt>
        <wp:comment_content><![CDATA[$comment.translatedBody]]></wp:comment_content>
        <wp:comment_approved>1</wp:comment_approved>
        <wp:comment_type></wp:comment_type>
        <wp:comment_parent></wp:comment_parent>
        <wp:comment_user_id></wp:comment_user_id>
        </wp:comment>
